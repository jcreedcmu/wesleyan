    ind {A} {B} f (Î· a) â†¦ f a
  {-# REWRITE I-Î² #-}

ğ”» : {A : Set} (a : A) â†’ Set
ğ”» {A} a = Î£ A (Î» b â†’ Î· a == Î· b)

